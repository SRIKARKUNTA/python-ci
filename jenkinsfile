pipeline {
    agent any
        stages{
            stage('checkout'){
                steps {
                  git branch: 'main', url: 'https://github.com/SRIKARKUNTA/python-ci.git'
                }
            }
             stage('build'){
                steps {
                   sh 'python /var/lib/jenkins/workspace/Python\\ ci/python.py "SRIKARKUNTA"'
                }
            }
      }
    }
